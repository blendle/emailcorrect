{
  "name": "emailcorrect",
  "version": "0.0.1",
  "description": "Test and auto correct e-mail addresses",
  "main": "./src/emailcorrect.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/*.js",
    "test:unit": "node --harmony --harmony_proxies ./node_modules/.bin/istanbul --colors --report=text --report=lcov cover ./node_modules/.bin/_mocha -- \"test/**/*.test.js\" --recursive --harmony_proxies --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "build:umd": "NODE_ENV=production webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blendle/emailcorrect.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "auto",
    "correct",
    "email",
    "test"
  ],
  "author": "Jesse Dijkstra <jesse@blendle.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blendle/emailcorrect/issues"
  },
  "homepage": "https://github.com/blendle/emailcorrect#readme",
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.9.2",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-polyfill": "^6.9.1"
  }
}
