{
  "name": "emailcorrect",
  "version": "0.0.1",
  "description": "Test and auto correct e-mail addresses",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/*.js",
    "test:unit": "node --harmony --harmony_proxies ./node_modules/istanbul/lib/cli.js --colors --report=text --report=lcov cover _mocha -- \"test/**/*.test.js\" --recursive --harmony_proxies --require ./test/setup.js",
    "test:watch": "mocha \"test/**/*.test.js\" --recursive --harmony_proxies --require ./test/setup.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blendle/emailcorrect.git"
  },
  "keywords": [
    "auto",
    "correct",
    "email",
    "test"
  ],
  "author": "Jesse Dijkstra <jesse@blendle.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blendle/emailcorrect/issues"
  },
  "homepage": "https://github.com/blendle/emailcorrect#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-polyfill": "^6.9.1"
  }
}
